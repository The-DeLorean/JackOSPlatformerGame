/**
 * Main class of the Pong game.
 */
class Main {

    /** Initializes a Pong game and starts running it. */
    function void main() {
        var PlatformerGame game;
        do PlatformerGame.newInstance();
        let game = PlatformerGame.getInstance();
        do game.run();
        do game.dispose();
        return;
    }
}
